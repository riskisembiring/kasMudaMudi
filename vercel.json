{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "api/*.php", "use": "now-php@1.0.0" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    },
    "api/*.php": {
      "runtime": "now-php@1.0.0"
    }
  }
}
